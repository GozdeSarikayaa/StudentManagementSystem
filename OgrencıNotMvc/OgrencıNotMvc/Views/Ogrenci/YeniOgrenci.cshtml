
@{
    ViewBag.Title = "YeniOgrenci";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>

<form method="post" class="form-group">
    <div>
        <label>Öğrenci Adı</label>
        <input type="text" class="form-control" name="OGRAD" />
    </div>
    <div style="margin-top:10px">

        <label>Öğrenci Soyad</label>
        <input type="text" class="form-control" name="OGRSOYAD" />
    </div>
    <div style="margin-top:10px">

        <label>Öğrenci Fotoğraf </label>
        <input type="text" class="form-control" name="OGRFOTO" />
    </div>


    <div style="margin-top:10px">

        <label>Öğrenci Cinsiyet</label>
        <input type="text" class="form-control" name="OGRCİNSİYET" />
    </div>


    <div style="margin-top:10px">

        <label>Öğrenci Kulup</label>
       
        <input type="text" class="form-control" name="OGRKULUP" />
    </div>

   
    

   

    

    <div style="margin-top:10px">

      <button class="btn btn btn-primary">Öğrenci Ekle </button>
    </div>


</form>

</div>